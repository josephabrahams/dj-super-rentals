{
  "name": "dj-super-rentals",
  "description": "A simple app to demonstrate how to integrate Django & Ember.",
  "repository": "https://github.com/josephabrahams/dj-super-rentals",
  "logo": "https://dj-super-rentals.herokuapp.com/static/assets/images/teaching.png",
  "keywords": ["Django", "Ember", "Ember.js"],
  "env":{
    "MAPBOX_ACCESS_TOKEN": {
      "description": "Mapbox Access Token, https://account.mapbox.com/access-tokens/",
      "required": true
    },
    "SECRET_KEY": {
      "generator": "secret"
    }
  },
  "addons":[
    "heroku-postgresql:hobby-dev"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "heroku/python"
    }
  ]
}
